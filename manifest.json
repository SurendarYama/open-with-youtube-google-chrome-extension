{
  "name": "Open With Youtube",
  "description": "Open Selected Text with YouTube Search",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "icons": {
    "16": "youtube-16.png",
    "48": "youtube-48.png",
    "128": "youtube-128.png"
  },
  "commands": {
    "open_with_youtube": {
      "suggested_key": {
        "default": "Ctrl+Y",
        "mac": "Command+Y"
      },
      "description": "Open selected text with YouTube search"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}